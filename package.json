      "@fastify/multipart": "^9.0.3",
      "@fastify/rate-limit": "^10.2.2",
      "@nestjs/apollo": "^13.0.3",
      "@nestjs/common": "^11.0.12",
      "@nestjs/config": "^4.0.1",
      "@nestjs/core": "^11.0.12",
