      "class-validator": "^0.14.1",
      "cloudinary": "^2.6.0",
      "dotenv": "^16.4.7",
      "fastify-gql-upload": "^1.0.0",
      "fastify-multipart": "^5.4.0",
      "fs": "0.0.1-security",
